ui_menu_file_for_finding_similar_books
	<- 	ui_user_command_class_atom; 
		ui_user_command_class_view_kb;
	=> nrel_main_idtf: 
		[Найти схожие книги]
			(* <- lang_ru;; *);
		[Find similar books]
			(* <- lang_en;; *);
	=> ui_nrel_command_template:
		[*
		question_finding_similar_books _-> ._question_finding_similar_books_instance
			(*
				_-> rrel_1:: ui_arg_1;;
			*);;
		._question_finding_similar_books_instance _<- question;;
		*];
	=> ui_nrel_command_lang_template: 
		[Запрос нахождения схожих книг: $ui_arg_1] 
			(* <- lang_ru;; *);
		[Request to find similar books: $ui_arg_1]
			(* <- lang_en;; *);;
